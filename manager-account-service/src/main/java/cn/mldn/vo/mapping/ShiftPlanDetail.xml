<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.mldn.dao.IShiftPlanDetailDAO">

	<insert id="doCreate" parameterType="ShiftPlanBase"
		useGeneratedKeys="true" keyProperty="shift_plan_detail_id">
		INSERT INTO shift_plan_detail (shift_plan_base_id,detail_name,start_time,end_time,days,exc_order) 
		VALUES(#{shift_plan_detail_id},#{shift_plan_base_id},#{detail_name},#{start_time},#{end_time},#{days},#{exc_order})
	</insert>
</mapper>